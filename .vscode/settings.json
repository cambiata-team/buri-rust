{
    "editor.formatOnSave": true,
    "[rust]": {
        "editor.defaultFormatter": "rust-lang.rust-analyzer"
    },
    "[markdown]": {
        "editor.formatOnSave": true
    },
    "git.branchProtection": ["main", "master"],
    "files.exclude": {
        "target": true
    },
    "rust-analyzer.checkOnSave.command": "cranky",
    "cSpell.words": [
        "canonicalize"
    ]
}
